

class Ball {
  
  int ballWidth;
  int ballHeight;
  int ballXpos;
  int ballYpos;
  int ballSpeed;
  
  Ball(int ballWidth, int ballHeight, int ballXpos, int ballYpos, int ballSpeed) {
    this.ballWidth= ballWidth;
    this.ballHeight = ballHeight;
    this.ballXpos = ballXpos;
    this.ballYpos = ballYpos;
    this.ballSpeed = ballSpeed;
  }
  
  
}